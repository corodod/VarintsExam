import java.util.Arrays;

/*
7.1
реализовать функцию которая из List<Integer> составит новый список List<Integer>,
который будет содержать элементы между первым и вторым значением 0,
закпрещено обращаться к элементам по индексу
 */
public class Main {
    public static void gwerty(int[][] aa){// замеянет последний и последний снулями столбцы
        int gg = areZeroHere(aa);
        for (int i = 0;i < aa.length; ++i) {
            int t = aa[i][gg];
            aa[i][gg] = aa[i][aa[0].length-1];
            aa[i][aa[0].length-1] = t;
        }
        System.out.println(Arrays.deepToString(aa));
    }

    public static int areZeroHere(int[][] aa){
        int gg = 0;
        for (int i = 1; i < aa.length; ++i) {
            for (int j = 0; j < aa[0].length; ++j) {
                if (aa[i][j] == 0) {
                    gg = Math.max(gg, i);
                }
            }
        }
        return gg;// максимальный столбец с нуликом
    }

    public static void main(String[] args) {
        int[][] array = {{4,10,54,55,6},{12,3,0,44,50},{50,0,21,33,34}};
        gwerty(array);
    }
}
