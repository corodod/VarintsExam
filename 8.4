import java.util.Arrays;
import java.util.Scanner;

/*
реализовать функцию, которая заполняет массив следующим образом:
5 4 3 2 1 0 1 2 3
6 5 4 3 2 1 0 1 2
7 6 5 4 3 2 1 0 1
8 7 6 5 4 3 2 1 0
 */
public class Main {
    public static void main(String[] args) {
        Scanner ss = new Scanner(System.in);
        int a = ss.nextInt();//длина строки
        int b = ss.nextInt();// количество строк
        int[][] gg = new int[b][a];
        for(int i = 0; i<b;i++){
            for(int j = 0; j<a;j++){//i j a b
                if(j-b-i>0){
                    gg[i][j] = j-(b+i+1);
                }else{
                    gg[i][j] = a+i-b-j;
                }
            }
        }
        System.out.println(Arrays.deepToString(gg));
    }
}
