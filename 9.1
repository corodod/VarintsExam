import java.util.ArrayList;
import java.util.List;

/*
реализовать функцию которая делает следующее
{1, 2, 3, 5, -1, 0, 5, 3} -> {3, 5, 8, 4, -1, 5, 8}
 */
public class Main {
    public static void main(String[] args) {
        int[] ss = {1, 2, 3, 5, -1, 0, 5, 3};
        List<Integer> list = new ArrayList<>();
        int c = 0;
        int var = 0;
        for(int f:ss){
            if(c==0){
                c+=1;
            }else{
                list.add(var+f);
            }
            var=f;
        }
        for(Integer i: list){
            System.out.print(i+" ");
        }
    }
}
