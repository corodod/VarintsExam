import java.util.Arrays;
import java.util.Scanner;

/*
реализовать функцию кототрая вощвращает переданный двумерный массив целых чисел по приведенному ниже шаблону
4 4 4 4 4 5 6 7
3 3 3 3 4 5 6 7
2 2 2 3 4 5 6 7
1 1 2 3 4 5 6 7
0 1 2 3 4 5 6 7
 */
public class Main {

    public static void massive1(int a, int b){
        int[][] massive = new int[b][a];
        /*
        a - длина массива
        b - высота массива(количество строк)
         */
        for(int i =0; i<b;i++){
            for(int j = 0; j<a;j++){
                if(j<b-i){
                    massive[i][j] = (b - 1-i);
                }else{
                    massive[i][j] = j;
                }
            }
        }
        System.out.println(Arrays.deepToString(massive));
    }
    public static void main(String[] args) {
        Scanner ss = new Scanner(System.in);
        int a = ss.nextInt();
        int b = ss.nextInt();
        massive1(a,b);
    }
}
